{
  "name": "go-game-vanilla",
  "version": "1.0.0",
  "description": "The ancient board game of Go written in Vanilla JS",
  "main": "src/main.ts",
  "scripts": {
    "test": "npx tsc && jest",
    "start": "npx tsc --watch & node --watch ./build/main.js"
  },
  "author": "Ed Fildes",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "pug": "^3.0.2",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/ramda": "^0.29.10",
    "prettier": "3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/tests/*.js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
